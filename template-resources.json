{
  "Resources": {
    "operatorPage": {
      "Type": "AWS::Serverless::Function",
      "Properties": {
        "Handler": "src/operator.page",
        "MemorySize": 512,
        "Events": {
          "API": {
            "Type": "Api",
            "Properties": {
              "Path": "/",
              "Method": "get"
            }
          }
        },
        "Description": "A Lambda function that returns a static string.",
        "Policies": [
          "AWSLambdaBasicExecutionRole"
        ]
      }
    },
    "checkLogin": {
      "Type": "AWS::Serverless::Function",
      "Properties": {
        "Handler": "src/operator.checkLogin",
        "MemorySize": 512,
        "Events": {
          "API": {
            "Type": "Api",
            "Properties": {
              "Path": "/auth",
              "Method": "get"
            }
          }
        },
        "Description": "A Lambda function that returns a static string.",
        "Policies": [
          "AWSLambdaBasicExecutionRole"
        ]
      }
    },
    "loginClient": {
      "Type": "AWS::Serverless::Function",
      "Properties": {
        "Handler": "src/operator.loginClient",
        "MemorySize": 512,
        "Events": {
          "API": {
            "Type": "Api",
            "Properties": {
              "Path": "/auth",
              "Method": "post"
            }
          }
        },
        "Description": "A Lambda function that returns a static string.",
        "Policies": [
          "AWSLambdaBasicExecutionRole"
        ]
      }
    },
    "helloFromLambdaFunction": {
      "Type": "AWS::Serverless::Function",
      "Properties": {
        "CodeUri": "./src/",
        "Handler": "auth.test",
        "Events": {
          "API": {
            "Type": "Api",
            "Properties": {
              "Path": "/myresource",
              "Method": "get"
            }
          }
        },
        "Description": "A Lambda function that returns a static string.",
        "Policies": [
          "AWSLambdaBasicExecutionRole"
        ]
      }
    }
  }
}